{
  "name": "norwegian-utils",
  "version": "0.1.6",
  "description": "Utilities used in our Norwegian applications.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -d",
    "prepush": "npm run format:check && npm run test && (cd login-app && npm run prepush)",
    "format": "prettier --write './**/*.js'",
    "format:check": "prettier -l './**/*.js'",
    "test": "rm **/*.js && jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unfold/norwegian-utils.git"
  },
  "author": "OBOS",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/unfold/norwegian-utils/issues"
  },
  "homepage": "https://github.com/unfold/norwegian-utils#readme",
  "devDependencies": {
    "@types/jest": "24.0.19",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  }
}
